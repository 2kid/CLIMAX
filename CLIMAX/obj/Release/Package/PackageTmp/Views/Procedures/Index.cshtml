@model IEnumerable<CLIMAX.Models.Procedure>

@{
    ViewBag.Title = "Procedures - Index";
}



<p>
    @Html.ActionLink("Create New", "Create", new { id = ViewBag.TreatmentID})
</p>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ProcedureName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StepNo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ProcedureName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StepNo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ProcedureID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ProcedureID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ProcedureID })
        </td>
    </tr>
}
</table>

<div>
    @Html.ActionLink("Back to List", "Index", "Treatments")
</div>
