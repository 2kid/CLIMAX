            @Html.DisplayFor(modelItem => item.ModeOfPayment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AmtPayment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GiftCertificateAmt)
        </td>
